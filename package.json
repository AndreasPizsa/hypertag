    "@commitlint/config-conventional": "^7.1.2",
    "ava": "^4.0.1",
    "benchmark": "^2.1.4",
    "cheerio": "^1.0.0-rc.2",
    "coveralls": "^3.0.2",
    "fast-html": "^0.1.2",
    "got": "^9.3.2",
